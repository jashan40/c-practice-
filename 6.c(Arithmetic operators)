// Working of arithmetic operators
#include <stdio.h>
int main()
{
    int a ,b , c;
    printf("Enter value of a,b:\n");
    scanf("%d%d",&a,&b);
    
    c = a+b;
    printf("a+b = %d\n",c);
    c = a-b;
    printf("a-b = %d\n",c);
    c = a*b;
    printf("a*b = %d\n",c);
    c = a/b;
    printf("a/b = %d\n",c);
    c = a%b;
    printf("Remainder is = %d \n",c);
    
    return 0;
}
