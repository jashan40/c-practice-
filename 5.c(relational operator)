//Relational operators
#include <stdio.h>

int main() {
    int a,b,c;
    printf("enter value of a,b,c:");
    scanf("%d%d%d",&a,&b,&c);
    
    
    printf("%d==%d is %d\n",a,b,a==b);
    printf("%d==%d is %d\n",a,c,a==c);
    printf("%d>%d is %d\n",a,b,a>b);
    printf("%d>%d is %d\n",a,c,a>c);
    printf("%d<%d is %d\n",a,b,a<b);
    printf("%d<%d is %d\n",a,c,a<c);
    printf("%d!=%d is %d\n",a,b,a!=b);
    printf("%d!=%d is %d\n",a,c,a!=c);
    printf("%d>=%d is %d\n",a,b,a>=b);
    printf("%d>=%d is %d\n",a,c,a>=c);
    printf("%d<=%d is %d\n",a,b,a<=b);
    printf("%d<=%d is %d\n",a,c,a<=c);
    

    return 0;
}
